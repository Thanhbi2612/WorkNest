# API Configuration
# Note: When using Docker with Nginx proxy, API calls go through Nginx
# So we use relative URLs instead of absolute URLs
VITE_API_URL=/api
VITE_SOCKET_URL=

# App Configuration
VITE_APP_NAME=Task Management
VITE_APP_VERSION=1.0.0

# Development
VITE_DEV_MODE=false

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
